                    <div class="box-body btn-block">
                        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                            <ol class="carousel-indicators">
                                <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                                <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
                                <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
                                <li data-target="#carousel-example-generic" data-slide-to="3" class=""></li>
                            </ol>
                            <div class="carousel-inner">
                                <div class="item active">
                                    @*<img src="~/resources/car-03.jpg" alt="Raffle" style="width:100%">*@
                                    <div class="item CarouselContainer">
                                        <img class="CarouselContainer" src="~/resources/malayan_1.jpg" alt="" style="width:100%" />

                                    </div>
                                    @*<div class="carousel-caption">
                            First Slide
                        </div>*@
                                </div>
                                <div class="item">
                                    @*<img src="~/resources/car-06.jpg" alt="Richmer" style="width:100%">*@
                                    <img src="~/resources/malayan_2.jpg" alt="" style="width:100%" />

