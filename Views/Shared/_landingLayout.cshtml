    <script>
        $('#fpwd').submit(function () {
            $(window).unbind('beforeunload');
        });
        $('#fpwd1').submit(function () {
            $(window).unbind('beforeunload');
        });
        $('#LoginModal').submit(function () {
            $(window).unbind('beforeunload');
        });
